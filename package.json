{
  "name": "solid-fetch",
  "version": "3.0.9",
  "description": "Http request client promise-based supporting injectable request properties like: headers, parameters, query, body, etc.",
  "type": "module",
  "main": "./dist/index.min.mjs",
  "module": "./dist/index.min.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.min.mjs",
      "require": "./dist/index.min.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "author": "uria1rogel@gmail.com",
  "license": "MIT",
  "keywords": [
    "fetch",
    "request-client",
    "http-request"
  ],
  "scripts": {
    "build": "vite build && yarn build:ts",
    "build:ts": "npx tsc --emitDeclarationOnly",
    "prepublishOnly": "yarn build",
    "eslint": "eslint ."
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "npm:eslint-plugin-i@^2.29.1",
    "rollup": "^2.46.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/u-rogel/solid-fetch.git"
  },
  "bugs": {
    "url": "https://github.com/u-rogel/solid-fetch/issues"
  },
  "homepage": "https://github.com/u-rogel/solid-fetch#readme",
  "dependencies": {
    "tslib": "^2.8.1"
  }
}
